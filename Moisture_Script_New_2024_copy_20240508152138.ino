void handleRoot() {
  server.send(200, "text/plain", "Hello from ESP8266!");
}

void handleSetThreshold() {
  if (server.hasArg("threshold")) {
    low_thredhold = server.arg("threshold").toInt();
    server.send(200, "text/plain", "Threshold set successfully");
  } else {
    server.send(400, "text/plain", "Bad Request");
  }
}

void handleSetExternalSensor() {
  if (server.hasArg("value")) {
    external_value = server.arg("value").toInt();
    server.send(200, "text/plain", "Sensor value set successfully");
  } else {
    server.send(400, "text/plain", "Bad Request");
  }
}




void handleGetMoisture() {
  String response = String(soilmoisturepercent);
  server.send(200, "text/plain", response);
}


void handleGetThreshold() {
  String response = String(low_thredhold);
  server.send(200, "text/plain", response);
}


void infoPanel(int lowthredhold, int moistureValue, int moisturePercentage, int delayTime)
{
  display.setCursor(lbl_curs_x, lbl_curs_y);
  display.print("RH:" + String(soilmoisturepercent));
  display.println(" %");
  display.setCursor(lbl_curs_x, lbl_curs_y+15);
  display.print("Raw:" + String(moistureValue));
  display.setCursor(lbl_curs_x, lbl_curs_y+30);
  display.print("L-Thr:" + String(lowthredhold));
  display.println(" %");
  display.setCursor(lbl_curs_x, lbl_curs_y+45);
  display.print("EXT:" + String(external_value));
  display.println(" %");
  display.display();
  delay(delayTime);

}